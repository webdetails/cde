      <Connection id="templateQueryTerritory" type="mondrian.jndi">
         <Catalog>mondrian:/SteelWheels</Catalog>
         <Jndi>SampleData</Jndi>
      </Connection>
      <Connection id="templateQueryCountry" type="mondrian.jndi">
