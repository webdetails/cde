            <Tooltip>Shows icons (symbols such as radio-buttons or input boxes) next to each item</Tooltip>
            <InputType>Boolean</InputType>
            <OutputType>Boolean</OutputType>
            <Order>64</Order>
            <Advanced>true</Advanced>
            <Version>1.0</Version>
          </Header>
        </DesignerProperty>

        <DesignerProperty>
          <Header>
