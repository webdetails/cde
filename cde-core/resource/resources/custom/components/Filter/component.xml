            <OutputType>Number</OutputType>
            <Order>63</Order>
            <Version>1.0</Version>
          </Header>
        </DesignerProperty>


        <DesignerProperty>
          <Header>
            <Name>showIcons</Name>
            <Parent>BaseProperty</Parent>
            <DefaultValue>"true"</DefaultValue>
